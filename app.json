{
  "expo": {
    "name": "LifeQuest",
    "slug": "lifequest-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1e3a5f"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lifequest.rn"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1e3a5f"
      },
      "package": "com.lifequest.rn"
    },
    "web": {
      "bundler": "metro"
    }
  }
}
